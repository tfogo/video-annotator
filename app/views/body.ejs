<div class="container">
  <div class="header">
    <h3 class="text-muted">Video Annotator <%= videoName %></h3>
  </div>

  <div class="row">
    <div class="col-sm-8">
      <video id="video" src="<%= videoName %>" autobuffer>
      </video>
      
      <div class="controls">
        
        <div class="play-btn">
          <i class="fa fa-play"></i>
        </div>

        <div class="bar">
          <div class="watched-bar"></div>
          <div class="buffered-bar"></div>
        </div>
        
        <div class="buttons">
          <div class="time">00:00</div>
          <div class="volume">
            <i class="fa fa-volume-down"></i>
          </div>
        </div>
        
      </div>
      
    </div>
  </div>

</div>
